#FIREBASE_AUTH_EMULATOR_HOST=127.0.0.1:9199 # .env.test example

GOOGLE_APPLICATION_CREDENTIALS=<SPECIFY_FIREBASE_SERVICE_ACCOUNT_JSON_PATH>
